    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fr.geringan.activdash.activities.ActivServerActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/activity_vertical_padding">

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/graphs_fragment_container_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:name="fr.geringan.activdash.fragments.GraphsFragment" />

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
